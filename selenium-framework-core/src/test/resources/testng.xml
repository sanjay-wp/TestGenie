<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="KPN Fresh Test Suite" parallel="tests" thread-count="1">
    <test name="Edge Tests">
        <parameter name="browser" value="edge"/>
        <classes>
            <class name="com.framework.sample.kpnfresh.tests.TestRunner"/>
        </classes>
    </test>
    
    <listeners>
        <listener class-name="com.framework.listeners.TestListener"/>
        <listener class-name="com.framework.listeners.CucumberListener"/>
    </listeners>
</suite> 